{
  "name": "secl-group/phpcs-secl-standard",
  "keywords": [
    "phpcs",
    "Secl",
    "coding standard",
    "maintainability"
  ],
  "homepage": "https://github.com/SECL-Group/phpcs-secl-standard",
  "description": "A PHP_CodeSniffer maintainability standard for Secl Group applications",
  "license": "BSD",
  "require": {
    "squizlabs/php_codesniffer": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "target-dir": "secl-group/phpcs/Secl",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
